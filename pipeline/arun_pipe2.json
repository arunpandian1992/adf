{
	"name": "arun_pipe2",
	"properties": {
		"activities": [
			{
				"name": "Get Metadata1",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "Arun_parameter",
						"type": "DatasetReference",
						"parameters": {
							"arun_sys": {
								"value": "@pipeline().parameters.pipe2_sys",
								"type": "Expression"
							},
							"arun_dir": {
								"value": "@pipeline().parameters.pipe2_input",
								"type": "Expression"
							},
							"arun_filename": {
								"value": "@pipeline().parameters.pipe2_filename",
								"type": "Expression"
							}
						}
					},
					"fieldList": [
						"itemName",
						"itemType",
						"exists",
						"childItems",
						"lastModified"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "DelimitedTextReadSettings"
					}
				}
			}
		],
		"parameters": {
			"pipe2_sys": {
				"type": "string",
				"defaultValue": "adfproject"
			},
			"pipe2_input": {
				"type": "string",
				"defaultValue": "input"
			},
			"pipe2_output": {
				"type": "string",
				"defaultValue": "output"
			},
			"pipe2_filename": {
				"type": "string"
			},
			"pipe2_archive": {
				"type": "string",
				"defaultValue": "archive"
			}
		},
		"folder": {
			"name": "self"
		},
		"annotations": []
	}
}