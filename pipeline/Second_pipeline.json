{
	"name": "Second_pipeline",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Source_df",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "parameter_ds",
						"type": "DatasetReference",
						"parameters": {
							"ds_file_system": {
								"value": "@pipeline().parameters.pp_file_system",
								"type": "Expression"
							},
							"ds_file_directory": {
								"value": "@pipeline().parameters.pp_file_output_dir",
								"type": "Expression"
							},
							"ds_file_name": {
								"value": "@pipeline().parameters.pp_file_name",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"pp_file_system": {
				"type": "string"
			},
			"pp_file_input_dir": {
				"type": "string"
			},
			"pp_file_output_dir": {
				"type": "string"
			},
			"pp_file_archive_dir": {
				"type": "string"
			},
			"pp_file_name": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Activity"
		},
		"annotations": []
	}
}